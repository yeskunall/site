{
  "name": "eminent-equinox",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "astro check && astro build",
    "dev": "astro dev",
    "format": "prettier --cache --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext astro,js,jsx,mjs,ts,tsx .",
    "preview": "astro preview",
    "spellcheck": "cspell \"src/content/post/**/*.md\"",
    "start": "astro dev",
    "sync": "astro sync",
    "upgrade-deps": "bunx npm-check-updates@latest --format group --interactive",
    "codegen": "graphql-codegen"
  },
  "dependencies": {
    "@astrojs/vercel": "7.8.0",
    "@urql/core": "5.0.3",
    "astro": "4.15.1",
    "date-fns": "3.6.0"
  },
  "eslintIgnore": [
    ".astro",
    ".github",
    ".vercel",
    "**/node_modules/",
    "/internal/",
    "/playwright/.cache/",
    "/playwright-report/",
    "src/generated/"
  ],
  "devDependencies": {
    "@astrojs/check": "0.9.3",
    "@astrojs/markdown-remark": "5.2.0",
    "@astrojs/mdx": "3.1.5",
    "@astrojs/rss": "4.0.7",
    "@astrojs/sitemap": "3.1.6",
    "@astrojs/tailwind": "5.1.0",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/typescript": "4.0.7",
    "@graphql-codegen/typescript-operations": "4.2.1",
    "@radix-ui/colors": "3.0.0",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/typography": "0.5.13",
    "@total-typescript/ts-reset": "0.5.1",
    "@types/eslint": "8.56.10",
    "@types/hast": "3.0.4",
    "@types/markdown-it": "14.1.1",
    "@types/sanitize-html": "2.11.0",
    "@typescript-eslint/eslint-plugin": "7.10.0",
    "@typescript-eslint/parser": "7.10.0",
    "astro-eslint-parser": "1.0.2",
    "astro-icon": "1.1.0",
    "autoprefixer": "10.4.19",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "cspell": "8.8.3",
    "cssnano": "7.0.1",
    "cssnano-preset-advanced": "7.0.1",
    "eslint": "8.57.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-astro": "1.2.0",
    "eslint-plugin-deprecation": "3.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-perfectionist": "2.10.0",
    "eslint-plugin-typescript-sort-keys": "3.2.0",
    "markdown-it": "14.1.0",
    "mdast-util-to-string": "4.0.0",
    "micromark": "4.0.0",
    "micromark-extension-gfm": "3.0.0",
    "postcss": "8.4.38",
    "postcss-import": "16.1.0",
    "prettier": "3.2.5",
    "prettier-plugin-astro": "0.13.0",
    "prettier-plugin-tailwindcss": "0.5.14",
    "prettier-plugin-toml": "2.0.1",
    "pretty-ms": "9.0.0",
    "reading-time": "1.5.0",
    "rehype-accessible-emojis": "0.3.2",
    "rehype-autolink-headings": "7.1.0",
    "rehype-external-links": "3.0.0",
    "remark-definition-list": "2.0.0",
    "remark-github-beta-blockquote-admonitions": "3.1.1",
    "remark-unwrap-images": "4.0.0",
    "sanitize-html": "2.13.0",
    "schema-dts": "1.1.2",
    "tailwind-merge": "2.3.0",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5",
    "unist-util-visit": "5.0.0"
  },
  "engines": {
    "node": "^20.3.0"
  }
}
