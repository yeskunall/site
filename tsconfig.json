{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["src/components/*"],
      "@/components/ui/*": ["src/components/ui/*"],
      "@/data/*": ["data/*"],
      "@/hooks": ["src/hooks/index.ts"],
      "@/layouts/*": ["src/layouts/*"],
      "@/lib/*": ["src/lib/*"],
      "@/site-config": ["src/site.config.ts"],
      "@/styles/*": ["src/styles/*"],
      "@/utils/*": ["utils/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "exclude": [".vscode", "dist", "node_modules"]
}
