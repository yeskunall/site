{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["src/components/*"],
      "@/content/*": ["src/content/*"],
      "@/data/*": ["data/*"],
      "@/layouts/*": ["src/layouts/*"],
      "@/lib/*": ["src/lib/*"],
      "@/site-config": ["src/site.config.ts"],
      "@/styles/*": ["src/styles/*"],
      "@/utils/*": ["utils/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "exclude": [".vscode", "dist", "node_modules"]
}
